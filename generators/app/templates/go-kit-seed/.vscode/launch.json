  "version": "0.2.0",
  "configurations": [
    {
      "name": "Remote",
      "type": "go",
      "request": "launch",
      "mode": "remote",
      "remotePath": "/go/src/<%= org %>/<%= appName %>",
      "port": 2345,
      "host": "127.0.0.1",
      "showLog": true,
      "program": "${workspaceRoot}",
      "envFile": "${workspaceRoot}/.env"
    }
  ]
}
